            <PasswordField fx:id="repetirNovaSenha" layoutX="152.0" layoutY="108.0" />
            <Separator layoutX="5.0" layoutY="49.0" prefHeight="9.0" prefWidth="305.0" />
            <Button fx:id="trocarSenhaBtn" layoutX="182.0" layoutY="156.0" mnemonicParsing="false" onAction="#trocarSenhaBtnActionPerformed" text="TROCAR SENHA">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Label layoutX="14.0" layoutY="14.0" text="SENHA ATUAL:">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="68.0" text="NOVA SENHA:">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="108.0" text="REPETIR:">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
